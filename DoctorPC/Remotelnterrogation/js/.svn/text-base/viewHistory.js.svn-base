var view = sessionStorage.getItem('viewHistory');
var back = sessionStorage.getItem('backonly');

$(function(){
	showView();
})

function showView(){
	if(view == 1 && back == 1){
		$('.thisBack1').hide();
		$('.thisBack2').show();
	}else{
		$('.thisBack2').hide();
		$('.thisBack1').show();
	}
}
