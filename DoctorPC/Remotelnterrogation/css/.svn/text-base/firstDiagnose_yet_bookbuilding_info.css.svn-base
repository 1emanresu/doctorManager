/*添加更多医嘱弹出框*/

.moreAdvice-modal-content {
	display: flex;
}

.modal-content-left {
	flex: 1;
}

.modal-content-right input {
	width: 25vw;
	border: 1px solid gainsboro;
	border-right-color: white;
	height: 30px;
	font-size: 14px;
}

.modal-content-right button {
	width: 9vw;
	margin-left: -4px;
	height: 30px;
}

.modal-content-right {
	flex: 1;
}

.modal-content-left textarea {
	width: 90%;
	border: none;
	height: 45vh;
	margin-left: 2vw;
	font-size: 14px;
	background-color: rgb(242, 242, 242);
}

.modal-content {
	padding-top: 40px;
	height: auto;
	width: 70vw;
	position: fixed;
	left: -8vw;
	background-color: rgb(242, 242, 242);
}

.advice-color-change {
	FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0, startColorStr=#2dd7c6, endColorStr=#1f67fa);
	/*IE*/
	background: -moz-linear-gradient(top, #2dd7c6, #1f67fa));
	/*火狐*/
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#2dd7c6), to(#1f67fa));
	/*谷歌*/
	color: white;
	height: 30px;
	border-radius: 5px;
	font-size: 14px;
	width: 120px;
	border: none;
	margin-right: 40px;
	margin-top: 5px;
}

.advice-color-no-change {
	border: 1px solid #666666;
	color: #666666;
	height: 30px;
	border-radius: 5px;
	font-size: 14px;
	width: 120px;
	margin-right: 40px;
	background-color: white;
	margin-top: 5px;
}

.button-div {
	margin-top: 20px;
	margin-left: 15vw;
}

.common-color {
	FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0, startColorStr=#2dd7c6, endColorStr=#1f67fa);
	/*IE*/
	background: -moz-linear-gradient(top, #2dd7c6, #1f67fa));
	/*火狐*/
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#2dd7c6), to(#1f67fa));
	/*谷歌*/
	color: white;
	font-size: 14px;
	border: none;
}

