/* 选项卡*/
.nva-tabs-ul {
	background-color: #EAEEF1;
}

.nva-tabs-ul li a {
	color: #000000;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	font-size: 12px;
}

.nav-tabs {
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}

.nva-tabs-ul li {
	width: 200px;
	text-align: center;
}

.right-Tab-Control {
	width: 93%;
	margin: 0 auto;
	border-radius: 5px;
	margin: 10px auto;
	
}
.li_line {
	width: 99%;
	height: 5px;
	margin: 0 auto;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	background-color: #EAEEF1;
}
li.active .li_line {
	background: linear-gradient(to right, #1f67fa, #2dd7c6);
}

/*右侧信息*/
.right_firm_container{
	margin-top: 10px;
	display: flex;
	float: right;
	margin-right: 7px;
}
.right_firm_container label{
	color: #5794e7;
	margin-left:17px;
}
/*患者表格列表*/
.treated_table{
	width: 100%;
}
.treated_table td{
	height: 30px;
	text-align: center;
	border: 1px solid #D7D7D7;
}
.btn-primary:hover{
	background-color: #3189FB!important;
	border: 1px solid #3189FB!important;
}
.btn-primary{
	background: #76B0FF;
	border: 1px solid #76B0FF!important;
}

.baseinfor_shang input,
.baseinfor_shang select{ 
	height: 25px;
	border-radius: 5px;
	border: 1px solid gray;
	background-color: transparent;
	transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
	font-family: "微软雅黑"!important;
}
input:focus,
select:focus{
	border-color: #66AFE9;
	outline: 0;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 8px rgba(102,175,266,0.6)!important;
	box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 8px rgba(102,175,233,0.6)!important;
}
::-webkit-input-placeholder{
	text-indent: 5px;
	font-size: 13px;
	color: #999999;
}
.nav-tabs>li>a{
	background-color: #EAEEF1;
}
.nav-tabs>li{
	border: none;
}
.dropdown-menu{
	background-color: white;
}
.dropdown-menu>li>a{
	color: #333;
}
.dropdown-menu>.active>a,
.dropdown-menu>.active>a:hover{
	color: white;
	background-color: #337AB7;
}
.dropdown-menu>li>a:focus, .dropdown-menu>li>a:hover{
	background-color: #F5F5F5;
	color: #333;
}
.pagination>.active>a,
.pagination>.active>a:hover{
	background-color: #428BCA;
	border-color: #428bca;
}
.btn,.btn-default{
	border-color: rgb(170,178,189)!important;
}
.btn-primary, 
.btn-primary.active,
 .btn-primary.disabled, 
 .btn-primary:active, 
 .btn-primary[disabled]{
 	background-color: #76B0FF!important;
 }
