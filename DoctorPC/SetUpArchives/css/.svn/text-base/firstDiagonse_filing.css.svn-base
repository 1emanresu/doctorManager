.class_send__hint_container {
	position: relative;
}
.head{
	width: 100%;
	height: 5px;
	background: linear-gradient(to right, #1f67fa, #2dd7c6);
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.class_send__hint {
	position: absolute;
	top: 50px;
	left: 300px;
	text-align: left;
	width: 690px;
	height: auto;
	background: white;
	border: 1px solid #EAEAEA;
	box-shadow: 2px;
	border-radius: 7px;
	
}

.img_send__hint_close {
	float: right;
	margin-top: 15px;
	margin-right: 15px;
	cursor: pointer;
}

.font_size_color {
	font-size: 14px;
	color: red;
}


.class_firstDiagnose_tab_personage_info {
	width: 100%;
	height: auto;
	border: 1px solid #E4E4E4;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	overflow: hidden;
	padding: 25px;
}

.d_firstDiagnose_title_personage_info {
	font-size: 15px;
	font-weight: bold;
	margin-bottom: 15px;
}

.d_firstDiagnose_div_float {
	float: left;
	width: 25%;
}

.t_firstDiagnose_td_width {
	width: 93px;
}

.t_firstDiagnose_td_input td {
	height: 35px;
}

.t_firstDiagnose_td_input td input,
select {
	width: 135px;
}

.t_firstDiagnose_td_select td {
	height: 35px;
}

.t_firstDiagnose_td_select td select {
	width: 67px;
}

.d_firstDiagnose_div_address {
	clear: both;
	width: 100%;
}

.select_firstDiagnose_width {
	width: 27%;
}

.select_firstDiagnose_width2 {
	width: 44.4%;
}

.td_firstDiagnos_container {
	width: 92.1%;
	white-space: nowrap
}

.div_firstDiagnose_High {
	height: 11px;
	clear: both;
}

.d_firstDiagnose_censusRegister {
	width: 100%;
	overflow: hidden;
}

.table_firstDiagnose_width {
	width: 25%;
	float: left;
}

.td_firstDiagnos_container textarea {
	width: 97%;
	height: 65px;
	resize: none;
}

.content_firstDiagnose_right {
	display: flex;
	width: 250px;
	float: right;
	margin-right: 3%;
	padding-bottom: 55px;
}

.cancel_btn button {
	width: 115px;
	border-radius: 5px;
	border: 1px solid;
	background: white;
	height: 31px;
	margin-right: 5px;
	outline: none;
}
.save_next button {
	width: 107px;
	color: white;
	border-radius: 5px;
	border: 1px solid #5794E7;
	background: #5794E7;
	height: 27px;
}
.save_next button:hover {
	background-color: dodgerblue;
}
.jump_next button {
	width: 107px;
	border-radius: 5px;
	border: 1px solid;
	background: white;
	height: 27px;
	margin-left: 25px;
}

