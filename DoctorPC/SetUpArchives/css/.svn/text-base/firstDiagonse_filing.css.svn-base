.hisk_firstDiagnose_container {
	text-align: left;
	width: 690px;
	height: auto;
	background: white;
	border: 1px solid #EAEAEA;
	box-shadow: 2px;
	border-radius: 7px;
	display: block;
}

.img_close2 {
	float: right;
	margin-top: 11px;
	margin-right: 15px;
	cursor: pointer;
}
/*#severeTell_firstDiagnos_close img{
	right: 15px;
}*/
.hisk_firstDiagnose_div {
	padding: 15px 15px 15px 33px;
}

.hint_firstDiagnose_character {
	margin: 7px 5px 7px 0px;
	color: #6fA0E4;
}
.choice_firstDiagnose_container{
	margin-top: 15px;
}
.choice_firstDiagnose_div {
	float: left;
	width: 47%;
	margin-left: 15px;
}

.hisk_firstDiagnose_content {
	padding: 11px;
	width: 100%;
	border-radius: 5px;
	background:#F4F6F8;
	border: 1px solid #c9c9c9;
}

.hisk_firstDiagnose_content p {
	font-size: 14px;
}

.hisk_firstDiagnose_content div {
	padding-left: 15px;
}

.class_send_remind_check input {
	height: 15px;
}
.class_send_remind_check label{
	color: #888;
	font-weight: normal;
}
.hisk_firstDiagnose_content div input{
  vertical-align:middle 
}
.p_firstDiagnose_size{
	font-size: 14px;
}
.choice_firstDiagnose_hisk{
	overflow: hidden;
}
.choice_firstDiagnose_left{
	float: left;
	width: 50%;
}
.choice_firstDiagnose_right{
	float: right;
	width: 20%;
}
.div_irstDiagnose_tall{
	height: 160px;
}
.bar{
	cursor: pointer;
}

/*上边框底部调整*/
.div_send_remind_back{
	border-radius: 7px;
	border-bottom-right-radius: 0px;
	border-bottom-left-radius: 0px;
}
.hisk_trans_content {
	height: 200px;	
}
.hisk_trans_content label{
	font-weight: normal;
}

/*微调首诊建档-发送提醒弹框关闭按钮hover样式*/
.closeMove,.img_close1c{
	opacity: 0.5!important;
}
.closeMove:hover,.img_close1c:hover{
	opacity: 1!important;
}
.img_send__hint_close button.close{
	margin-top: -10px!important;
	margin-right: -7px!important;
}
