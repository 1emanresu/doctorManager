.advice-input{
	height: 280px;
	width: 100%;
	border: 1px solid #DDDDDD;
	border-top: none;
	margin-top: -10px;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
}
.inputs{
	width: 60%;
	height: 250px;
	border-right: 1px solid #DDDDDD;
	position: relative;
	float: left;
}
.thisAdvice_content{
	width: 100%;
	height: 250px;
	padding: 15px 12px;
	overflow: auto;
}
.input-img{
	width: 97%;
	height: 30px;
	line-height: 30px;
	position: absolute;
	bottom: 10px;
	text-align: right;
}
.input-img a{
	color: #B8BFC8;
	text-decoration: none;
	cursor: pointer;
}
.input-img a:hover{
	color: #82aeed;
}
.inputs-right{
	height:280px ;
	width: 40%;
	float: right;

}
.row{
	margin-top: 10px;
	margin-left: 1%;
	margin-right: 1%;
}
#search_box{
	border-radius: 1px;
	border: 1px solid #98bff2;
}
#search{
	background: #78a9ed;
	border: 1px solid #78A9ED;
	border-radius: 0px;
}
#search:hover{
	background: #3166ff;
	border: 1px solid #3166ff;
	color: white;
}
.right-span{
	width: 92%;
	height: 55%;
	border:none;
	border-top: 1px solid #D8D8D8;
	margin-left: 4%;
	margin-top: 8px;
	padding: 12px 0 12px 0;
	overflow: auto;
	position: relative;
}
#dbclick{
	position: absolute;
	bottom: 0;
	float: right;
	right: 0;
	margin-right: 1%;
}

