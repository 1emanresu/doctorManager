/*--------------------------------------------------------------------------------*/

.button_div button{
	float: left;
	width: 130px;
	margin: 10px 30px 0 0!important;
	font-family: "微软雅黑";
}
.button_div button:focus{
	background-color: dodgerblue;
}
.remind_manage{
	padding: 20px 70px;
}
#add:focus{
	background-color: dodgerblue;
}
.remind_list{
	width: 100%;
	height: auto;
	margin-top: 30px;
}
.list_title{
	width: 100%;
	height: 55px;
	border-radius: 5px 5px 0 0;
	background-color: rgba(233,236,236,1);
	line-height: 55px;
	text-align: center;
}
.list_name,
.list_operation{
	float: left;
	width: 14%;
	height: 55px;
	font-size: 14px;
}
.list_content{
	float: left;
	width: 70%;
	height: 55px;
	font-size: 14px;
}
.list_operation a{
	cursor: pointer;
}
.list_body{
	width: 100%;
	height: auto;
	line-height: 55px;
	text-align: center;
}
.body_content{
	height: 55px;
	border-bottom: 1px solid lightgray;
}


/*-------------------------------发送提醒记录框------------------------------------------------------*/
.col{
	display: flex;
	width: 100%;
	margin-bottom: 15px;
	/*height: 45px;*/

}
.col_title{
	float: left;
	width: 20%;
	margin-right: 15px;
	text-align: right;
	font-size: 14px;
}
.col_content{
	float: left;
	width: 75%;
	font-size: 14px;
}
.r_test{
	display: flex;
	width: 100%;
	padding-bottom: 10px;
}
.test_content{
	float: left;
	width: 80%;
	font-size: 14px;
}
.test_title{
	float: left;
	width: 15%;
	margin-right: 5px;
	font-size: 14px;
}
.send{
	float: left;
	width: 70px;
	height: 25px;
	margin-right: 20px;
	border-radius: 5px;
	background-color: #F2F2F2;
	line-height: 25px;
	text-align: center;
}
.button{
	width: 100%;
	padding-top: 10px;
	text-align: center;
}
#send_again{
	width: 100px;
	height: 30px;
	background-color: #5794E7;
	border: 1px solid #5794E7;
	border-radius: 5px;
	color: white;
}
/*----------------------------------------------------------------------------------------------------------*/