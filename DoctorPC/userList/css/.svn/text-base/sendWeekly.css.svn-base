* {
	padding: 0px;
	margin: 0px;
	font-size: 12px;
	color: #696969;
}

.class_main {
	width: 100%;
}

.top_Nav {
	margin-top: 7px;
	line-height: 30px;
	padding-left: 12px;
	border-bottom: gainsboro solid 1px;
}

.pro,
.pro2,
.pro3,
.pro4,
.pro5,
.pro6,
.pro7{
	width: 100%;
	padding: 1px 20px;
	clear: both;
	padding-right: 0px;
}
.pro4{
	margin-bottom: 40px;
}
.pro2 table tr td {
	text-align: center;
	border: gainsboro solid 1px;
	line-height: 30px;
}

.pro_left {
	float: left;
	width: 50%;
	display: none;
}

.pro_right {
	width: 50%;
	float: right;
	display: none;
}

.pro3_left {
	width: 49%;
	float: left;
	height: 350px;
	border: gainsboro solid 1px;
	border-radius: 5px;
	padding: 5px 10px;
}

.pro3_right {
	width: 50%;
	height: 350px;
	float: right;
}

.select_div {
	border: white solid 1px;
	height: 35px;
}

.select_div ul {
	padding-left: 0;
}

.select_div ul li {
	list-style: none;
	float: left;
	line-height: 35px;
	color: gainsboro;
	font-size: 14px;
}

.span {
	margin: 0 10px;
}
#butt1{
	text-align: center;
}
#butt1 .but1,#butt1 .but3{
	margin-top: 15px;
	margin-left: 10px;
	border: none;
	border-radius: 5px;
	width: 130px;
	line-height: 28px;
	color: white;
	background: #76b0ff;
}
 
.but1:hover,
.but3:hover{
	background-color: #3589fb!important;
}
/*#butt1 .but2:hover{
	border: 1px solid #404D5B;
}*/

#butt1 .but2 {
	margin-left: 10px;
	border: gainsboro solid 1px;
	border-radius: 5px;
	width: 130px;
	line-height: 28px;
	background: white;
	margin-bottom: 15px;
}

#butt2 {
	float: right;
}

#butt2 .but3 {
	outline: none;
	margin-top: 15px;
	margin-left: -10px;
	border: none;
	border-radius: 20px;
	width: 130px;
	line-height: 23px;
	color: white;
	background: #8CB6EF;
}

#butt2 .but4 {
	outline: none;
	margin-top: 15px;
	margin-left: 10px;
	border: none;
	border-radius: 20px;
	width: 130px;
	line-height: 23px;
	color: white;
	background: #8CB6EF;
}

.img_firstDiagnose_position {
	position: absolute;
	top: 9px;
	right: 15px;
	cursor: pointer;
}

.close {
	opacity: 1 !important;
}

.advice-table1 {
	margin: 15px;
}

.advice-table1 td {
	border-right: 1px solid rgb(204, 204, 204);
	padding: 0 10px;
	color: rgb(204, 204, 204);
}

#search_keyword{
	margin-left: 15px;
}
.advice-table1 td:last-child {
	border: none;
	padding-right: 0px;
}

.advice-table1 td:first-child {
	padding-left: 0px;
}
*{
	outline: none;
}

.advice-table1 td:hover{
	color: #404D5B;
}
/*发送报告基础信息的样式*/
.person-Message{
	padding:1px 20px;
	height: auto;
	margin-top: 20px;
	margin-bottom: 10px;
}
.label-person{
	margin-bottom: 10px;
}
.label-person span{
	margin-right: 19%;
}
.label-person label{
	margin-right: 15px;
}
.div-text label{
	float: left;
}
.div-text div{
	float: right;
	height: 75px;
	width: 93%;
	border: 1px solid #BCBCBC;
}	
.more{
	height: auto;
	margin-top: 90px;
}
.span-img{
	float: right;
	width: 100%;
	margin-bottom: 10px;
	cursor: pointer;
}
.hadmore{
	float: right;
	width: 5%;
}
.flip{
	transform: rotate(180deg);
	-ms-transform: rotate(180deg);          /* IE 9 */
	-webkit-transform: rotate(180deg);      /* Safari and Chrome */
	-o-transform: rotate(180deg);           /* Opera */
	-moz-transform: rotate(180deg);         /* Firefox */
}
.more-span{
	display: none;
	width: 93%;
	float: right;
}
.span-con{
	height: 24px;
	line-height: 24px;
}
.more-span label{
	margin-right: 10px;
}
.more-span span{
	margin-right: 20%;
}
.history1{
	margin-left: 18%;
}
.morethen:hover{
	color: #169BD5;
}
/*end*/

/*院检报告样式*/
.checkProjectHaveDone{
	height: auto;
}
.div-check,.self-testing{
	float: left;
	width: 32%;
	height: auto;
	border-right: 1px solid #D7D7D7;
	margin-right: 2%;
	margin-bottom: 2%;
}
.symptom{
	float: left;
	width: 32%;
	height: auto;
}
.div-check span,.div-check1 span,.div-check2 span{
	color: #1F6ACF;
	font-size: 14px;
}
.div-check1 img,.div-check img{
	float: right;
	width: 8%;
}
.div-table1,.div-table2,.div-table3{
	width: 95%;
	height: 30px;
	margin-bottom: 20px;
}
.table1,.table2,.table3{
	height: auto;
	width: 95%;
	border: 1px solid #666666;
}
.table1 td,.table2 td,.table3 td{
	width: 33.3%;
	height: 30px;
}
.div-here{
	height: 20px;
}
/*异常指标分析start*/
.con{
	height: 200px!important;
	width: 95%;
}
