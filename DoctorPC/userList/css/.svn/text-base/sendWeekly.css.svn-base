/*弹出窗*/
#selfTest-box,#checkProject-box,#echarts2-box{
	width: 450px;
	border: 1px solid #E4E4E4;
	padding: 20px 5px 0px 20px;
	line-height: 20px;
	position: absolute!important;
	top: 10%;
	left: 20%;
	background-color: white;
	border-radius: 5px;
	box-shadow: gray;
	box-shadow: 1px 1px 10px rgba(0, 0, 0, 0.349019);
	display: none;
}
/*通用弹出 框关闭按钮*/
.close-move {
	text-align: center;
	width: 25px;
	padding: 2px;
	float: right;
	position: relative;
	top: -10px;
	cursor: pointer;
	opacity: 0.6;
	z-index: 99;
	top: -13px;
}

.close-move:hover {
	opacity: 0.9;
}
