/*主div*/

.class_main {
	min-width: 1024px;
	/*max-width: 1440px;*/
	margin: 10px auto;
}

.class_main select {
	border-radius: 5px;
	background-color: transparent;
	transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}

.class_main input {
	border-radius: 5px;
	background-color: transparent;
	transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}

.main_content {
	margin: 0px auto;
	/*min-width: 1024px;*/
	width:95%;
	padding: 0 20px;
}


/*头部搜索栏*/

.content_search {
	margin: 35px 0;
	display: flex;
	min-width: 80%;
}


/*头部路径*/

