/*通用样式---zhangqt*/
/*设置全部内容为不可选择*/
* {
	moz-user-select: -moz-none;
	-moz-user-select: none;
	-o-user-select: none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

input,
select,
button {
	outline: none;
}

* {
	padding: 0px;
	margin: 0px;
	font-size: 12px;
	color: #696969;
}

.pointer {
	cursor: pointer;
}

a {
	text-decoration: none !important;
}

label {
	font-weight: normal;
}

