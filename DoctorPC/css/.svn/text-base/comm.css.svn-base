#sendPeopleAndWay div,
#sendPeopleAndWay2 div {
	margin: 0 auto!important;
}

#sendPeopleAndWay div button,
#sendPeopleAndWay2 div button {
	margin-top: 20px;
}

#sendingButton,
#sendingButton2 {
	text-align: center;
}

#sendingButton button,
#sendingButton2 button {
	width: 130px;
	line-height: 30px;
	height: 30px;
	border-radius: 3px;
	background-color: white;
	outline: none;
	border: none;
	border: 1px solid gainsboro;
}

#cancel,
#cancel2 {
	margin-left: 20px;
}

#sendPeopleAndWay div span button,
#sendPeopleAndWay2 div span button {
	width: 100px;
	line-height: 30px;
	height: 30px;
	margin-left: 20px;
	border-radius: 3px;
	background-color: white;
	outline: none;
	border: none;
	border: 1px solid gainsboro;
}

#checkProjecButton button,
#checkProjecButton2 button {
	padding-left: 10px;
	line-height: 25px;
	height: 25px;
	margin-left: 10px;
	border-radius: 3px;
	background-color: white;
	outline: none;
	cursor: default;
	border: none;
	border: 1px solid gainsboro;
}

#checkProjecButton,
#checkProjecButton2 {
	margin-right: 10px;
}

.prompt {
	margin-top: 5px;
}

#div-button button,
#div-button2 button {
	height: 25px;
	margin-top: 17px;
	padding-left: 10px;
	margin-right: 8px;
	border-radius: 3px;
	background-color: white;
	outline: none;
	cursor: default;
	border: none;
	border: 1px solid gainsboro;
}

#shortMessage,
#shortMessage2 {
	margin-left: 8px!important;
}

.box-top-middle {
	float: left;
}

.rotate-img {
	/* Safari */
	-webkit-transform: rotate(-180deg);
	/* Firefox */
	-moz-transform: rotate(-180deg);
	/* Internet Explorer 滤镜写法*/
	filter: progid: DXImageTransform.Microsoft.BasicImage(rotation=3);
}

#index-top {
	width: 11px;
	height: 13px;
	margin-left: 5px;
}

.selfTest-title {
	margin-top: 8px;
}

.addCheckProject {
	width: 95%;
}

#circleForSelfTest,
#circleForHCheck {
	margin-top: 10px;
	margin-left: 120px;
}

.btn-primary:hover {
	background-color: dodgerblue!important;
	border-color: #dodgerblue!important;
}
#addjd:hover{
	background-color: #5794E7!important;
}
.btn-primary {
	background-color:#76b0ff;
	border: none;
}
.btn-primary:hover{
	background-color: #3189FBimportant;
}

.nav-tabs>li.active>a,
.nav-tabs>li.active>a:focus,
.nav-tabs>li.active>a:hover {
	border: 1px solid transparent;
}

.nav-tabs {
	border: none;
}

#comfirmSending,
#comfirmSending2 {
	background-color: #76b0ff !important;
	border: none!important;
	margin-left: 75px;
	color: white;
}
#comfirmSending:hover, 
#comfirmSending2:hover{
	background-color: #3189FB!important;
}
#checkProjecButton button img,
#checkProjecButton2 button img,
#div-button button img,
#div-button2 button img {
	float: right;
	cursor: pointer;
}

#div-button button {
	display: none;
}

#div-button button img,
#div-button2 button img {
	width: 12px;
	margin-left: 3px;
	margin-top: -1px;
	margin-right: 1px;
}

.report-table {
	margin-top: 10px;
	width: 320px !important;
}

.report-table tr td {
	line-height: 23px;
	border: gainsboro solid 1px;
	text-align: center;
}

/*查看报告中的表格样式*/

#ls-table {
	position: relative;
	top: -20px;
	width: 95%;
}
#sendPeopleAndWay button img,
#sendPeopleAndWay2 button img{
	margin-left: -20px;
	margin-right: 8px;
	margin-top: -3px;
}
